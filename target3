Cell[CellGroupData[{

Cell["Deifnitions", "Subsection",ExpressionUUID->"9642b1a4-cca0-40a6-bf29-83f6f7332a81"],

Cell[BoxData[
 RowBox[{
  RowBox[{"ineq", "=", 
   RowBox[{
    RowBox[{
     RowBox[{
      RowBox[{"x", "^", "2"}], "+", 
      RowBox[{"y", "^", "2"}]}], "\[LessEqual]", 
     RowBox[{"a", "^", "2"}]}], "&&", 
    RowBox[{"y", "\[GreaterEqual]", 
     RowBox[{"a", " ", 
      RowBox[{"Cos", "[", 
       RowBox[{"q", "/", "2"}], "]"}]}]}]}]}], ";"}]], "Input",ExpressionUUID-\
>"f082b55a-c97a-4b8c-a084-c7ca7e1e98a4"],

Cell[BoxData[
 RowBox[{
  RowBox[{"reg", "=", 
   RowBox[{"RegionIntersection", "[", 
    RowBox[{
     RowBox[{"ImplicitRegion", "[", 
      RowBox[{
       RowBox[{"y", ">", 
        RowBox[{"a", " ", 
         RowBox[{"Cos", "[", 
          RowBox[{"q", "/", "2"}], "]"}]}]}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "y"}], "}"}]}], "]"}], ",", 
     RowBox[{"Disk", "[", 
      RowBox[{
       RowBox[{"{", 
        RowBox[{"0", ",", "0"}], "}"}], ",", "a", ",", 
       RowBox[{
        RowBox[{"Pi", "/", "2"}], "+", 
        RowBox[{
         RowBox[{"{", 
          RowBox[{
           RowBox[{"-", "1"}], ",", "1"}], "}"}], " ", 
         RowBox[{"q", "/", "2"}]}]}]}], "]"}]}], "]"}]}], ";"}]], "Input",Expr\
essionUUID->"e783a38a-649c-4d16-87c3-6c17239bba3d"],

Cell[BoxData[
 RowBox[{
  RowBox[{"assum", "=", 
   RowBox[{
    RowBox[{"a", ">", "0"}], "&&", 
    RowBox[{"0", "<", "q", "<", "\[Pi]"}]}]}], ";"}]], "Input",ExpressionUUID-\
>"c5df8396-83c5-4b5d-b12f-7578f615188a"]
}, Open  ]],

